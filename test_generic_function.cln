functions:
	Any identity(Any value)
		return value

start()
	integer x = identity(42)
	print "Identity function with integer: " + x
	
	string text = identity("Hello World")
	print "Identity function with string: " + text
	
	float f = identity(3.14159)
	print "Identity function with float: " + f
	
	boolean b = identity(true)
	print "Identity function with boolean: " + b
