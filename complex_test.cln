// Complex test file demonstrating Clean Language features

// Function that computes the factorial of a number
function integer factorial()
    input
        integer n
    if n <= 1
    return 1
    else
    return n * factorial(n - 1)

// Function that sums all numbers from start to end
function integer sumRange()
    input
        integer start
        integer end
    integer total = 0
    integer i = start
  
    iterate i in start to end
    total = total + i
  
  return total

// Function to check if a number is prime
function boolean isPrime()
    input
        integer n
    if n <= 1
    return false
    
    integer i = 2
    while i * i <= n
        if n % i == 0
      return false
    i = i + 1
  
  return true

// Main program entry point
function start()
    integer a = 5
    integer b = 10
  
    integer factA = factorial()
    integer rangeSum = sumRange()
    boolean isBPrime = isPrime()
  
    // Print results
    println("Factorial of {a}: {factA}")
    println("Sum from {a} to {b}: {rangeSum}")
    println("Is {b} prime: {isBPrime}") 