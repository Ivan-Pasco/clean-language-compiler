start()
	// Test StringUtils methods
	string text = "Hello World"
	integer length = StringUtils.length(text)
	string upper = StringUtils.toUpper(text)
	string lower = StringUtils.toLower(text)
	string trimmed = StringUtils.trim("  hello  ")
	boolean starts = StringUtils.startsWith(text, "Hello")
	boolean ends = StringUtils.endsWith(text, "World")
	integer index = StringUtils.indexOf(text, "World")
	string sub = StringUtils.substring(text, 0, 5)
	string replaced = StringUtils.replace(text, "World", "Universe")
	Array<string> parts = StringUtils.split(text, " ")
	
	// Test ArrayUtils methods
	Array<integer> numbers = [1, 2, 3, 4, 5]
	integer arrayLength = ArrayUtils.length(numbers)
	Array<integer> sliced = ArrayUtils.slice(numbers, 1, 3)
	string joined = ArrayUtils.join(parts, "-")
	Array<integer> reversed = ArrayUtils.reverse(numbers)
	Array<integer> sorted = ArrayUtils.sort([5, 2, 8, 1, 9])
	Array<integer> pushed = ArrayUtils.push(numbers, 6)
	integer popped = ArrayUtils.pop(numbers)
	
	// Test MathUtils methods
	float sum = MathUtils.add(5.5, 3.2)
	float difference = MathUtils.subtract(10.0, 4.0)
	float product = MathUtils.multiply(3.0, 4.0)
	float quotient = MathUtils.divide(15.0, 3.0)
	integer remainder = MathUtils.modulo(10, 3)
	float absolute = MathUtils.abs(-5.5)
	float squareRoot = MathUtils.sqrt(16.0)
	float rounded = MathUtils.round(3.7)
	float minimum = MathUtils.min(5.0, 3.0)
	float maximum = MathUtils.max(5.0, 3.0)
	float power = MathUtils.pow(2.0, 3.0)
	float clamped = MathUtils.clamp(15.0, 0.0, 10.0)
	
	println:
		"Standard library test completed!" 