/**
 * Hello World Example for Clean Language
 * 
 * This example demonstrates a simple program that prints "Hello, World!"
 * and returns an integer value.
 */

// Main entry point function
start() {
    // Print hello world to the console with a newline
    printl "Hello, World!"
    
    // A simple calculation
    let value = 40 + 2
    
    // Print the result
    printl "The answer is: ${value}"
    
    // Return a value
    return value
} 