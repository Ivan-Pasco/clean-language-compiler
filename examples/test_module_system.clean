// Test program for the module system implementation
// Demonstrates all import patterns from the specification

import:
    TestModule                    // whole module
    MathUtils as Math            // module alias  
    TestModule.add               // single symbol
    MathUtils.sqrt as msqrt      // symbol alias

functions:
    void start()
        // Test whole module import
        integer result1 = TestModule.add(5, 3)
        print("TestModule.add(5, 3) = ")
        print(result1)
        
        // Test module alias
        integer result2 = Math.max(10, 7)
        print("Math.max(10, 7) = ")
        print(result2)
        
        // Test single symbol import
        integer result3 = add(15, 25)  // Direct call without module prefix
        print("add(15, 25) = ")
        print(result3)
        
        // Test symbol alias
        number result4 = msqrt(16)     // Using alias for MathUtils.sqrt
        print("msqrt(16) = ")
        print(result4)
        
        // Test string function from module
        string greeting = TestModule.greet("World")
        print(greeting) 