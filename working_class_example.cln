class Person
	string name
	integer age
	
	constructor(string personName, integer personAge)
		name = personName
		age = personAge
	
	functions:
		string getName()
			return name
		
		integer getAge()
			return age

start()
	Person john = Person("John Doe", 30)
	string name = john.getName()
	integer age = john.getAge()
	print "Name:"
	print name
	print "Age:"
	print age