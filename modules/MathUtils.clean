// MathUtils - Mathematical utility functions for testing module system

function integer max(integer a, integer b)
	integer result = 0
	if a > b
		result = a
    else
		result = b
	return result

function integer min(integer a, integer b)
	if a < b then return a else return b

function float sqrt(float x)
	return x ^ 0.5

function integer abs(integer x)
	if x < 0 then return -x else return x 