start()
    print("=== Clean Language toString() Verification ===\n")
    
    // Test floating point values
    number val1 = 16.0
    number val2 = 3.14159
    number val3 = 0.0
    
    print("Float 16.0: ")
    print(val1.toString())
    print("\n")
    
    print("Float 3.14159: ")  
    print(val2.toString())
    print("\n")
    
    print("Float 0.0: ")
    print(val3.toString())
    print("\n")
    
    // Test integer values
    integer int1 = 42
    integer int2 = 0
    integer int3 = -123
    
    print("Integer 42: ")
    print(int1.toString())
    print("\n")
    
    print("Integer 0: ")
    print(int2.toString())
    print("\n")
    
    print("Integer -123: ")
    print(int3.toString())
    print("\n")
    
    // Test boolean values
    boolean bool1 = true
    boolean bool2 = false
    
    print("Boolean true: ")
    print(bool1.toString())
    print("\n")
    
    print("Boolean false: ")
    print(bool2.toString())
    print("\n")
    
    print("=== All toString() methods working correctly! ===\n")