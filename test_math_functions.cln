function start()
    // Test basic trigonometric functions
    real pi_over_4 = 0.7853981633974483
    real sin_result = sin(pi_over_4)
    print("sin(π/4) = " + sin_result.toString())
    
    real cos_result = cos(pi_over_4)
    print("cos(π/4) = " + cos_result.toString())
    
    real tan_result = tan(pi_over_4)
    print("tan(π/4) = " + tan_result.toString())
    
    // Test inverse trigonometric functions
    real half = 0.5
    real asin_result = asin(half)
    print("asin(0.5) = " + asin_result.toString())
    
    real acos_result = acos(half)
    print("acos(0.5) = " + acos_result.toString())
    
    real atan_result = atan(1.0)
    print("atan(1.0) = " + atan_result.toString())
    
    // Test hyperbolic functions
    real one = 1.0
    real sinh_result = sinh(one)
    print("sinh(1.0) = " + sinh_result.toString())
    
    real cosh_result = cosh(one)
    print("cosh(1.0) = " + cosh_result.toString())
    
    real tanh_result = tanh(one)
    print("tanh(1.0) = " + tanh_result.toString())
    
    // Test exponential functions
    real exp2_result = exp2(2.0)
    print("2^2 = " + exp2_result.toString())
    
    // Test power function
    real pow_result = power(2.0, 3.0)
    print("2^3 = " + pow_result.toString())
    
    print("Mathematical functions test completed!") 