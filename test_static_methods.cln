class MathUtils
	functions:
		float add(float a, float b)
			return a + b
			
		float multiply(float a, float b)
			return a * b

class Person
	string name
	integer age
	
	constructor(name, age)
	
	functions:
		string getInfo()
			return "Person: " + name
		
		// Static helper method
		boolean isValidAge(integer age)
			return age >= 0

start()
	// Test static method calls
	result1 = MathUtils.add(5.0, 3.0)
	result2 = MathUtils.multiply(4.0, 2.0)
	
	// Test mixed static and instance calls
	person = Person("Alice", 25)
	info = person.getInfo()
	valid = Person.isValidAge(30)
	
	print("Math result 1: " + result1)
	print("Math result 2: " + result2)
	print("Person info: " + info)
	print("Valid age: " + valid) 