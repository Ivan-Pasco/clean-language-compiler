// Generic array test
functions:
	Array<Any> makeArray()
		input
			Any item1
			Any item2
		Array<Any> result
		result.push(item1)
		result.push(item2)
		return result

start()
	// Create integer array
	Array<integer> numbers = makeArray(100, 200)
	print "Integer array elements: " + numbers[0] + ", " + numbers[1]
	
	// Create string array
	Array<string> words = makeArray("hello", "world")
	print "String array elements: " + words[0] + ", " + words[1]
