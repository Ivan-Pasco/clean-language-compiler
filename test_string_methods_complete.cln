function start()
	// Test the newly implemented string methods
	
	string text = "  Hello World  "
	println("Original text: '" + text + "'")
	
	// Test trimStart()
	string trimmed_start = text.trimStart()
	println("After trimStart(): '" + trimmed_start + "'")
	
	// Test trimEnd()
	string trimmed_end = text.trimEnd()
	println("After trimEnd(): '" + trimmed_end + "'")
	
	// Test full trim (already implemented)
	string fully_trimmed = text.trim()
	println("After trim(): '" + fully_trimmed + "'")
	
	// Test substring with bounds
	string sub1 = fully_trimmed.substring(0, 5)
	println("Substring(0, 5): '" + sub1 + "'")
	
	string sub2 = fully_trimmed.substring(6)
	println("Substring(6): '" + sub2 + "'")
	
	// Test replace
	string original = "Hello World Hello"
	string replaced = original.replace("Hello", "Hi")
	println("Replace 'Hello' with 'Hi': '" + replaced + "'")
	
	// Test lastIndexOf
	integer last_pos = original.lastIndexOf("Hello")
	println("Last index of 'Hello': " + string(last_pos))
	
	integer first_pos = original.indexOf("Hello")
	println("First index of 'Hello': " + string(first_pos))
	
	// Test padStart
	string short_text = "5"
	string padded = short_text.padStart(5, "0")
	println("PadStart '5' to length 5 with '0': '" + padded + "'")
	
	string name = "Clean"
	string padded_name = name.padStart(10, "* ")
	println("PadStart 'Clean' to length 10 with '* ': '" + padded_name + "'")
	
	// Test edge cases
	string empty = ""
	string trimmed_empty = empty.trim()
	println("Empty string trimmed: '" + trimmed_empty + "'")
	
	string single_char = "A"
	string sub_single = single_char.substring(0, 1)
	println("Single char substring: '" + sub_single + "'")
	
	println("All string method tests completed!") 