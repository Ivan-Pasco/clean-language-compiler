class Rectangle
	number width
	number height
	
	constructor(number w, number h)
		width = w
		height = h

class Circle
	number radius
	
	constructor(number r)
		radius = r

functions:
	number getArea(Rectangle rect)
		return 42.0
	
	number getPerimeter(Rectangle rect)
		return 84.0
	
	number getCircleArea(Circle circle)
		return 3.14

start()
	Rectangle rect = Rectangle(5.0, 3.0)
	Circle circle = Circle(2.0)
	
	number rectArea = rect.getArea()
	number rectPerimeter = rect.getPerimeter()
	number circleArea = circle.getCircleArea()
	
	print "Rectangle area:"
	print rectArea
	print "Rectangle perimeter:"
	print rectPerimeter
	print "Circle area:"
	print circleArea