	void testHttpPut()
		string putUrl = "https://api.example.com/users/123"
		string putBody = "{\"name\": \"Jane\"}"
		string putResponse = Http.put(putUrl, putBody)
		println("PUT request completed")

	void testHttpPatch()
		string patchUrl = "https://api.example.com/users/123"
		string patchBody = "{\"email\": \"new@example.com\"}"
		string patchResponse = Http.patch(patchUrl, patchBody)
		println("PATCH request completed")

	void testHttpDelete()
		string deleteUrl = "https://api.example.com/users/123"
		string deleteResponse = Http.delete(deleteUrl)
		println("DELETE request completed")

