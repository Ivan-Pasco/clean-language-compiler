start()
	// Comprehensive test of all list operations
	list<integer> numbers = [3, 1, 4, 1, 5]
	
	print "Original list length:"
	print List.length(numbers)
	
	// Test push
	List.push(numbers, 9)
	print "After push(9):"
	print List.length(numbers)
	
	// Test insert
	integer success = List.insert(numbers, 2, 2)
	print "Insert(2, 2) success:"
	print success
	print "After insert:"
	print List.length(numbers)
	
	// Test pop
	integer popped = List.pop(numbers)
	print "Popped:"
	print popped
	print "After pop:"
	print List.length(numbers)
	
	// Test remove
	integer removed = List.remove(numbers, 1)
	print "Removed from index 1:"
	print removed
	print "After remove:"
	print List.length(numbers)
	
	// Test sort (should sort the remaining elements)
	List.sort(numbers)
	print "After sort:"
	print List.length(numbers)