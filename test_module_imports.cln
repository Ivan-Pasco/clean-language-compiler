// Test module import functionality according to specification

// Test different import syntax patterns
import:
    Math                # whole module
    Math.sqrt           # single symbol
    String as S         # module alias
    List.length as len  # symbol alias

start()
    print "Testing module imports"
    
    // Test whole module usage
    number result = Math.sqrt(16.0)
    print "Math.sqrt(16.0) = " + result.toString()
    
    // Test single symbol usage  
    number result2 = sqrt(25.0)
    print "sqrt(25.0) = " + result2.toString()
    
    // Test module alias
    string upper = S.toUpperCase("hello")
    print "S.toUpperCase('hello') = " + upper
    
    // Test symbol alias
    list<integer> numbers = [1, 2, 3]
    integer size = len(numbers)
    print "len([1, 2, 3]) = " + size.toString()